{
  "name": "RaftCore",
  "version": "1.7.8",
  "description": "Core elements for the Raft ESP32 framework",
  "keywords": "esp32, esp32s3, framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/robdobsn/RaftCore"
  },
  "authors": [
    {
      "name": "Rob Dobson",
      "email": "rob@dobson.com",
      "url": "https://robdobson.com",
      "maintainer": true
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/robdobsn/RaftCore",
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "platforms": [
    "*"
  ],
  "build": {
    "srcDir": "components",
    "includeDir": "components",
    "libLDFMode": "deep+",
    "libArchive": false,
    "flags": [
      "-DJSMN_PARENT_LINKS",
      "-DJSMN_STRICT",
      "-Icomponents/comms/CommsBridgeMsg",
      "-Icomponents/comms/CommsChannelMsg",
      "-Icomponents/comms/CommsChannels",
      "-Icomponents/comms/CommsCoreIF",
      "-Icomponents/comms/FileStreamProtocols",
      "-Icomponents/comms/ProtocolBase",
      "-Icomponents/comms/ProtocolExchange",
      "-Icomponents/comms/ProtocolOverAscii",
      "-Icomponents/comms/ProtocolRawMsg",
      "-Icomponents/comms/ProtocolRICFrame",
      "-Icomponents/comms/ProtocolRICJSON",
      "-Icomponents/comms/ProtocolRICSerial",
      "-Icomponents/comms/RICRESTMsg",
      "-Icomponents/comms/ROSSerial",
      "-Icomponents/core/APICommon",
      "-Icomponents/core/ArduinoUtils",
      "-Icomponents/core/ArPreferences",
      "-Icomponents/core/Bus",
      "-Icomponents/core/ConfigPinMap",
      "-Icomponents/core/DebounceButton",
      "-Icomponents/core/DebugGlobals",
      "-Icomponents/core/DNSResolver",
      "-Icomponents/core/ExpressionEval",
      "-Icomponents/core/FileSystem",
      "-Icomponents/core/LEDPixels",
      "-Icomponents/core/libb64",
      "-Icomponents/core/Logger",
      "-Icomponents/core/MiniHDLC",
      "-Icomponents/core/MQTT",
      "-Icomponents/core/NamedValueProvider",
      "-Icomponents/core/NetworkSystem",
      "-Icomponents/core/NumericalFilters",
      "-Icomponents/core/RaftCoreApp",
      "-Icomponents/core/RaftJson",
      "-Icomponents/core/RestAPIEndpoints",
      "-Icomponents/core/RingBuffer",
      "-Icomponents/core/StatusIndicator",
      "-Icomponents/core/SupervisorStats",
      "-Icomponents/core/SysManager",
      "-Icomponents/core/SysMod",
      "-Icomponents/core/SysTypes",
      "-Icomponents/core/ThreadSafeQueue",
      "-Icomponents/core/Utils"
    ],
    "srcFilter": [
    ]
  }
}
