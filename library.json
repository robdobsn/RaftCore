{
  "name": "RaftCore",
  "version": "0.5.5",
  "description": "Core elements for the Raft ESP32 framework",
  "keywords": "esp32, esp32s3, framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/robdobsn/RaftCore"
  },
  "authors": [
    {
      "name": "Rob Dobson",
      "email": "rob@dobson.com",
      "url": "https://robdobson.com",
      "maintainer": true
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/robdobsn/RaftCore",
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "platforms": [
    "espressif32"
  ],
  "build": {
    "srcDir": "components/core",
    "includeDir": "components/core",
    "libLDFMode": "deep+",
    "libArchive": false,
    "extraScript": "lib_script.py",
    "flags": [
      "-DJSMN_PARENT_LINKS",
      "-DJSMN_STRICT",
      "-Icomponents/core/APICommon",
      "-Icomponents/core/ArduinoUtils",
      "-Icomponents/core/ArPreferences",
      "-Icomponents/core/Bus",
      "-Icomponents/core/CommsBridgeMsg",
      "-Icomponents/core/CommsChannelManager",
      "-Icomponents/core/CommsChannelMsg",
      "-Icomponents/core/CommsCoreIF",
      "-Icomponents/core/Config",
      "-Icomponents/core/ConfigPinMap",
      "-Icomponents/core/DebounceButton",
      "-Icomponents/core/ExpressionEval",
      "-Icomponents/core/FileStreamProtocols",
      "-Icomponents/core/FileSystem",
      "-Icomponents/core/HWElem",
      "-Icomponents/core/libb64",
      "-Icomponents/core/Logger",
      "-Icomponents/core/MiniHDLC",
      "-Icomponents/core/MQTT",
      "-Icomponents/core/NetCoreIF",
      "-Icomponents/core/NetworkSystem",
      "-Icomponents/core/NumericalFilters",
      "-Icomponents/core/ProtocolBase",
      "-Icomponents/core/ProtocolExchange",
      "-Icomponents/core/ProtocolOverAscii",
      "-Icomponents/core/ProtocolRawMsg",
      "-Icomponents/core/ProtocolRICFrame",
      "-Icomponents/core/ProtocolRICJSON",
      "-Icomponents/core/ProtocolRICSerial",
      "-Icomponents/core/RestAPIEndpoints",
      "-Icomponents/core/RICRESTMsg",
      "-Icomponents/core/RingBuffer",
      "-Icomponents/core/StatusIndicator",
      "-Icomponents/core/SupervisorStats",
      "-Icomponents/core/SysManager",
      "-Icomponents/core/SysMod",
      "-Icomponents/core/SysTypes",
      "-Icomponents/core/ThreadSafeQueue",
      "-Icomponents/core/Utils"
    ]
  }
}
